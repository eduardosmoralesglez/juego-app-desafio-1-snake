<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="500.0" spacing="5.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.puerto.juego1.controller.RecuperarController">
   <children>
      <Text fx:id="textEmail" strokeType="OUTSIDE" text="Correo Electronico" wrappingWidth="200.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Text>
      <TextField fx:id="emailTextRecuperar" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="16.0" prefWidth="200.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <Text fx:id="textMensaje" strokeType="OUTSIDE" strokeWidth="0.0">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Text>
      <Button fx:id="onRecuperarButton" mnemonicParsing="false" onAction="#onRecuperarClick" text="Aceptar">
         <padding>
            <Insets bottom="8.0" left="30.0" right="30.0" top="8.0" />
         </padding>
      </Button>
      <Button fx:id="onRecuperarBackToLoginButton" mnemonicParsing="false" onAction="#onClickBackToLogin" text="Volver a Login">
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
