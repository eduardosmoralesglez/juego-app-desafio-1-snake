<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="328.0" prefWidth="349.0" spacing="5.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.puerto.juego1.controller.RegistroController">
   <children>
      <Text fx:id="textUsuario" strokeType="OUTSIDE" strokeWidth="0.0" text="Usuario" />
      <TextField fx:id="textFieldUsuario" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="24.0" prefWidth="200.0" promptText="Introducir usuario">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <Text fx:id="textContrasenia" strokeType="OUTSIDE" strokeWidth="0.0" text="Contraseña" />
      <PasswordField fx:id="textFieldPassword" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="24.0" prefWidth="200.0" promptText="Introducir contraseña" />
      <Text fx:id="textContraseniaRepetir" strokeType="OUTSIDE" strokeWidth="0.0" text="Repetir Contrasenia" />
      <PasswordField fx:id="textFieldPasswordRepetir" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="24.0" prefWidth="200.0" promptText="Introducir contraseña" />
      <Text fx:id="textName" strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre" />
      <TextField fx:id="textFieldNombre" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="24.0" prefWidth="200.0" promptText="Introducir nombre" />
      <Text fx:id="textEmail" strokeType="OUTSIDE" strokeWidth="0.0" text="Correo Electronico" />
      <TextField fx:id="textFieldEmail" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="24.0" prefWidth="300.0" promptText="Introducir Correo Electronico" />
      <Text fx:id="textEmailRepetir" strokeType="OUTSIDE" strokeWidth="0.0" text="Repetir Correo Electronico" />
      <TextField fx:id="textFieldEmailRepetir" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="24.0" prefWidth="300.0" promptText="Introducir Correo Electronico" />
      <Text fx:id="textMensaje" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Button fx:id="buttonRegistrar" mnemonicParsing="false" onAction="#onClickRegistrar" text="Registrar" />
      <Button fx:id="onRegistroBackToLoginButton" mnemonicParsing="false" onAction="#onClickBackToLogin" text="Volver a Login">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
